library(gridExtra)
# Crear tabla con tutilo
crearTabla <- function(ttable,stitle) {
table <- tableGrob(ttable)
grid.newpage()
h <- grobHeight(table)
w <- grobWidth(table)
title <- textGrob(stitle, y=unit(0.5,"npc") + 0.5*h,
vjust=1, gp=gpar(fontsize=20))
gt <- gTree(children=gList(table, title))
grid.draw(gt)
}
